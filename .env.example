# .env.example â€” minimal env variables for Apps Script + PWA
# Copy this file to `.env` and replace placeholders with real values. Do NOT commit `.env`.

# Apps Script deployment ID (short id)
APPS_SCRIPT_DEPLOYMENT_ID=REPLACE_WITH_DEPLOYMENT_ID

# Full Apps Script web app exec URL (used as manifest.share_target.action)
APPS_SCRIPT_WEB_APP_URL=REPLACE_WITH_WEB_APP_EXEC_URL

# Optional secret token (used only for execute-as-me fallback). Keep secret out of repo.
SHARE_SECRET=REPLACE_WITH_LONG_SECRET_TOKEN

# Google Sheet used as the data store
SHEET_ID=REPLACE_WITH_GOOGLE_SHEET_ID

# Teacher account email used for domain-auth checks in Apps Script
TEACHER_EMAIL=teacher@example.edu
